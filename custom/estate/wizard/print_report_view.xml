<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Add view for wizard -->
    <record id="view_print_report" model="ir.ui.view">
        <field name="name">Print Report</field>
        <!-- <field name="model">print.report</field> -->
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="amount"/>
                </group>
                <footer>
                    <button name="print" class="btn-primary" type="object" string="Print"/>
                    <button string="No" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    
</odoo>